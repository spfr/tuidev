// Dual Agent Layout - nvim + 2 AI agent terminals
layout {
    tab name="Dev" {
        pane split_direction="vertical" {
            pane size="60%" {
                name "Editor"
                command "nvim"
                focus true
            }
            pane split_direction="horizontal" size="40%" {
                pane {
                    name "Agent-1"
                }
                pane {
                    name "Agent-2"
                }
            }
        }
    }
    tab name="Terminal" {
        pane {
            name "Shell"
        }
    }
}
